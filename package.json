{
  "name": "mantra-matui-redux",
  "private": true,
  "scripts": {
    "builddeploy": "eslint . && ./.dockerbuilddeploy",
    "lint": "eslint .",
    "pretest": "npm run lint --silent",
    "start": "meteor run --settings ./.config/settings-development.json"
  },
  "eslintConfig": {
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended"
    ],
    "settings": {
      "import/resolver": "meteor"
    },
    "parser": "babel-eslint",
    "rules": {
      "no-param-reassign": [0]
    }
  },
  "dependencies": {
    "mantra-core": "^1.7.0",
    "material-ui": "^0.15.1",
    "meteor-node-stubs": "~0.2.0",
    "mobx": "^2.3.3",
    "mobx-react": "^3.4.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-komposer": "^1.11.0",
    "react-mounter": "^1.2.0",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-meteor": "^0.2.4",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-meteor": "^3.6.0",
    "eslint-plugin-react": "^5.2.2"
  }
}
